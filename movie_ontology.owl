<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/thoma/ontologies/2020/3/f21bd#"
     xml:base="http://www.semanticweb.org/thoma/ontologies/2020/3/f21bd"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/thoma/ontologies/2020/3/f21bd">
        <dc:contributor>Thomas Di Martino</dc:contributor>
        <dc:creator>Thomas Di Martino</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">04.2020</dc:date>
        <dc:description>Movie ontology for the CourseWork nÂ°2 of F21BD - Heriot Watt</dc:description>
    </owl:Ontology>
    


